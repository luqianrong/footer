<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#f5f5f5">
    <com.example.footer.widget.EaseTitleBar
        android:id="@+id/find_title"
        android:layout_width="fill_parent"
        android:layout_height="44dp"/>
    <View
        android:layout_width="fill_parent"
        android:layout_height="0.5dp"
        android:layout_marginTop="10dp"
        android:background="#d8d8d8"/>
    <RelativeLayout
        android:id="@+id/rl_shop"
        android:layout_width="fill_parent"
        android:layout_height="50dp"
        android:background="#ffffff"
        >
        <ImageView
            android:id="@+id/img_shop"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_alignParentLeft="true"
            android:layout_marginLeft="12dp"
            android:background="@drawable/find_mall"/>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_toRightOf="@+id/img_shop"
            android:layout_marginLeft="10dp"
            android:textColor="#333333"
            android:textSize="16dp"
            android:text="商城"/>
        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="12dp"
            android:background="@drawable/find_right"/>
    </RelativeLayout>
    <View
        android:layout_width="fill_parent"
        android:layout_height="0.5dp"
        android:background="#d8d8d8"/>
    <RelativeLayout
        android:id="@+id/rl_game"
        android:layout_width="fill_parent"
        android:layout_height="50dp"
        android:background="#ffffff"
        >
        <ImageView
            android:id="@+id/img_game"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_alignParentLeft="true"
            android:layout_marginLeft="12dp"
            android:background="@drawable/find_games"/>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_toRightOf="@+id/img_game"
            android:layout_marginLeft="10dp"
            android:textColor="#333333"
            android:textSize="16dp"
            android:text="游戏"/>
        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="12dp"
            android:background="@drawable/find_right"/>
    </RelativeLayout>
    <View
        android:layout_width="fill_parent"
        android:layout_height="0.5dp"
        android:background="#d8d8d8"/>
    <RelativeLayout
        android:id="@+id/rl_wx"
        android:layout_width="fill_parent"
        android:layout_height="50dp"
        android:background="#ffffff"
        >
        <ImageView
            android:id="@+id/img_wx"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_alignParentLeft="true"
            android:layout_marginLeft="12dp"
            android:background="@drawable/find_literature"/>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_toRightOf="@+id/img_wx"
            android:layout_marginLeft="10dp"
            android:textColor="#333333"
            android:textSize="16dp"
            android:text="文学"/>
        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="12dp"
            android:background="@drawable/find_right"/>
    </RelativeLayout>
    <View
        android:layout_width="fill_parent"
        android:layout_height="0.5dp"
        android:background="#d8d8d8"/>
    <RelativeLayout
        android:id="@+id/rl_xianzhi"
        android:layout_width="fill_parent"
        android:layout_height="50dp"
        android:background="#ffffff"
        >
        <ImageView
            android:id="@+id/img_xianzhi"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_alignParentLeft="true"
            android:layout_marginLeft="12dp"
            android:background="@drawable/find_idle"/>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_toRightOf="@+id/img_xianzhi"
            android:layout_marginLeft="10dp"
            android:textColor="#333333"
            android:textSize="16dp"
            android:text="闲置"/>
